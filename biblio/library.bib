journal = {Chinese Journal of Aeronautics},
keywords = {Cascades,Highly loaded axial compressors,Loading limit,Single blades,Tandem blades},
month = {apr},
number = {4},
pages = {165--175},
publisher = {Elsevier},
title = {{Using tandem blades to break loading limit of highly loaded axial compressors}},
volume = {35},
year = {2022}
